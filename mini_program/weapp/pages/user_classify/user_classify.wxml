<view id="list">
    <view class="item" wx:for="{{arr}}" wx:key="arr" data-medical_name="{{item.MedicalName}}" data-address="{{item.Addr}}" data-hospital_name="{{item.HospitalName}}" bindtap="goDetailsPage">
        <image class="img" src="https://7072-prod-9gy59jvo10e0946b-1310014865.tcb.qcloud.la/a11smile/images/hospital_icon/nanchang.jpg?sign=35ef8f99ae02343bd0758f6711f3472a&t=1652624190"></image>
        <image class="touxiang" src="{{item.IconUrl}}"></image>
        <view class="hospital">{{item.HospitalName}}</view>
        <view class="line"></view>
        <view class="name">{{item.MedicalName}}</view>
        <view class="year">
            <text>年份不限</text>
        </view>
        <view class="count">{{item.Account}}</view>
        <view class="price">{{item.Min}}~{{item.Max}}AS</view>
    </view>
</view>

<view id="pearch"></view>