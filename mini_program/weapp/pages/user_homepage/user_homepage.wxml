<view id="inform">
    <view id="inform_img">
        <image src="https://7072-prod-9gy59jvo10e0946b-1310014865.tcb.qcloud.la/a11smile/images/icon/u117.svg?sign=f3f768505ccc501df22d5d4a1829efaf&t=1649081097"></image>
    </view>
    <view id="inform_txt">
        <text>紧急征询心脏病病历!</text>
    </view>
</view>
<view id="list">
        <view class="item" wx:for="{{pageData}}" wx:key="pageData" data-medical_name="{{item.MedicalName}}" data-address="{{item.Addr}}" bindtap="goDetailsPage">
            <view class="title">
                <text>{{item.MedicalName}}</text>
            </view>
            <view class="price">
                <text>{{item.Min}}~{{item.Max}}AS</text>
            </view>
            <view class="num">
                {{item.Account}}份
            </view>
            <view class="hospital">
                <view class="list_img">
                    <image src="https://7072-prod-9gy59jvo10e0946b-1310014865.tcb.qcloud.la/a11smile/images/hospital_icon/u13.jpg?sign=8c8622de247e47a08710ff0fbfc4e920&t=1649136066"></image>
                </view>
                <view class="name">
                    <text>{{item.HospitalName}}</text>
                </view>

            </view>
            <view class="address">
                <text>南昌 东湖区</text>
            </view>
        </view>
</view>
<view id="perch"></view>