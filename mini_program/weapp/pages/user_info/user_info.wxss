#head{
    width: 100%;
    height: 300rpx;
    display: flex;
    color: white;
}

#background{
    width: 100%;
    height: 300rpx;
    position: fixed;
    background-size: 100% 100%;
    z-index: -1;
}

#name{
    font-size: x-large;
    font-weight: 800;
}

#portrait{
    margin-left: 50rpx;
    margin-top: 50rpx;
}

#portrait image{
    width: 200rpx;
    height: 200rpx;
}

#mess{
    margin-top: 100rpx;
    margin-left: 30rpx;
}

#list image{
    width: 60rpx;
    height: 60rpx;
}

.item{
    display: flex;
    margin-left: 50rpx;
    margin-top: 100rpx;
    font-size: large;
    font-weight: 800;
}

#change{
    margin-top: 150rpx;
}

#change button{
    background-color: #0c86efb0;
    color: white;
}

.address{
    font-size: small;
    font-weight: 800;
    margin-left: 55rpx;
}