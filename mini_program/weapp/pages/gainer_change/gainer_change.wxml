<view id="change_portrait" bindtap="changeProtrait">
    <view id="p_txt">
        <text>头像</text>
    </view>
    <view id="portrait">
        <image src="{{protrait}}"></image>
    </view>
    <view id="p_icon">
        <image src="https://7072-prod-9gy59jvo10e0946b-1310014865.tcb.qcloud.la/a11smile/images/icon/u2439.svg?sign=f0a3166c3c251fe4858a89b0fac4b2ba&t=1649583253"></image>
    </view>
</view>

<view id="list">
    <view class="item" bindtap="changeResume">
        <view class="title">
            <text>企业简介</text>
        </view>
        <view class="content">
            <text>{{info}}</text>
        </view>
        <view class="icon">
            <image src="https://7072-prod-9gy59jvo10e0946b-1310014865.tcb.qcloud.la/a11smile/images/icon/u2439.svg?sign=f0a3166c3c251fe4858a89b0fac4b2ba&t=1649583253"></image>
        </view>
    </view>
</view>